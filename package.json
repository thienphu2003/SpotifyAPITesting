{
    "name": "express-typeorm-boilerplate",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest --collectCoverage",
        "build": "tsc",
        "start": "yarn serve",
        "serve": "node dist/index.js",
        "dev": "nodemon",
        "watch-ts": "tsc -w",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "migration:generate": "npm run typeorm migration:generate src/typeorm/migrations/$npm_config_name -- -d src/typeorm/data-source.ts",
        "migration:create": "npm run typeorm migration:create src/typeorm/migrations/$npm_config_name -- -d src/typeorm/data-source.ts",
        "migration:run": "npm run typeorm migration:run -- -d src/typeorm/data-source.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d src/typeorm/data-source.ts"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/mailgun-js": "^0.22.18",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.8",
        "tsconfig-paths": "^4.2.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.583.0",
        "@babel/core": "^7.24.6",
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-joi-validator": "^2.0.1",
        "express-jwt": "^8.4.1",
        "express-winston": "^4.2.0",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "nodemon": "^3.1.1",
        "pg": "^8.11.5",
        "prettier": "^3.2.5",
        "qs": "^6.12.3",
        "reflect-metadata": "^0.2.2",
        "spotify-web-api-node": "^5.0.2",
        "ts-node": "^10.9.2",
        "tslint": "^6.1.3",
        "tslint-config-airbnb": "^5.11.2",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0",
        "typescript": "^4.1.3",
        "uuid": "^9.0.1",
        "winston": "^3.13.0"
    }
}
